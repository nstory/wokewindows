<% pdf_url = "https://wokewindows-data.s3.amazonaws.com/pax_centurion_issues/#{filename}" %>
<% img_url = "https://wokewindows-data.s3.amazonaws.com/pax_centurion_covers/#{filename.sub(/\.pdf/, '.png')}" %>

<div class="col-6 col-md-4 col-lg-3">
  <div class="text-center">
    <a href="<%= pdf_url %>">
      <img class="img-fluid" src="<%= img_url %>">
    </a>
  </div>
  <div class="text-center mb-3">
    <a href="<%= pdf_url %>"><%= issue %></a>
  </div>
</div>
